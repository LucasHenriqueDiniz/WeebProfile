{
  "name": "weeb-profile-core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx tsx src/index.ts",
    "setup-fonts": "npx tsx src/setup/setupFonts.ts",
    "generate-readme": "npx tsx src/setup/readme/generateReadme.ts",
    "generate-action": "npx tsx src/setup/action/generateAction.ts",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "postinstall": "npx patch-package -y",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "align-text": "^1.0.2",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "puppeteer": "^23.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "string-width": "4.2.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/align-text": "^1.0.4",
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^8.2.0",
    "@types/image-to-base64": "^2.1.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "globals": "^15.11.0",
    "image-to-base64": "^2.2.0",
    "mustache": "^4.2.0",
    "npm-only-allow": "^1.2.6",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
