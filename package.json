{
  "name": "weeb-profile",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "svg-generator",
    "weeb-plugins",
    "weeb-dashboard",
    "weeb-debug-tool"
  ],
  "scripts": {
    "vercel:dev": "pnpm --filter weeb-dashboard dev",
    "vercel:start": "pnpm --filter weeb-dashboard start",
    "vercel:build": "pnpm --filter weeb-dashboard build",
    "build": "pnpm --filter weeb-dashboard build",
    "dev": "pnpm --filter weeb-dashboard dev:all",
    "start": "pnpm --filter weeb-dashboard start",
    "check:lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "check:lint-fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "check:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "check:prettier-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "format": "pnpm check:prettier-fix && pnpm check:lint-fix",
    "build:deps": "pnpm --filter @weeb/weeb-plugins build && pnpm --filter @weeb/svg-generator build",
    "dev:deps": "concurrently \"pnpm --filter @weeb/weeb-plugins dev\" \"pnpm --filter @weeb/svg-generator dev\"",
    "dev:svg-generator": "pnpm --filter svg-generator dev:server",
    "dev:watch": "pnpm --filter weeb-dashboard dev:watch",
    "dev:all": "concurrently \"pnpm --filter svg-generator dev:server\" \"pnpm --filter weeb-dashboard dev\"",
    "dev:preview": "concurrently -n \"watch,debug\" -c \"cyan,magenta\" \"pnpm --filter svg-generator watch:previews --plugin=myanimelist\" \"pnpm --filter svg-generator dev:debug --plugin=myanimelist --section=statistics\"",
    "dev:preview:all": "concurrently -n \"watch,debug\" -c \"cyan,magenta\" \"pnpm --filter svg-generator watch:previews:all\" \"pnpm --filter svg-generator dev:debug\"",
    "dev:debug": "concurrently -n \"debug-tool\" -c \"magenta\" --kill-others-on-fail \"pnpm --filter @weeb/debug-tool dev\"",
    "dev:debug-tool": "pnpm --filter @weeb/debug-tool dev",
    "build:debug-tool": "pnpm --filter weeb-debug-tool build"
  },
  "dependencies": {
    "@octokit/graphql": "^8.1.1",
    "@octokit/rest": "^21.0.2",
    "axios": "^1.7.7",
    "axios-cache-interceptor": "^1.6.1",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "octokit": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.3.1",
      "@types/react-dom": "^18.3.1"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "globals": "^13.24.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.4",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.4.0"
  }
}
