{
  "name": "@weeb/weeb-plugins",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./plugins": "./dist/plugins/index.js",
    "./plugins/metadata": "./dist/plugins/metadata.js",
    "./plugins/manager": "./dist/plugins/manager.js",
    "./plugins/tags": "./dist/plugins/tags.js",
    "./styles": "./dist/styles/index.js",
    "./styles/server": "./dist/styles/server.js",
    "./themes": "./dist/themes/index.js",
    "./themes/theme-utils": "./dist/themes/theme-utils.js",
    "./templates": "./dist/templates/index.js"
  },
  "dependencies": {
    "@octokit/graphql": "^8.1.1",
    "@octokit/rest": "^21.0.2",
    "@types/glob": "^9.0.0",
    "bottleneck": "^2.19.5",
    "country-flag-icons": "^1.6.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.22",
    "glob": "^13.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "pnpm validate && pnpm generate-metadata && pnpm generate-types && pnpm generate-section-previews && pnpm generate-tailwind-css && pnpm generate-style-index && tsup && pnpm copy-shared-css && pnpm verify-no-relative-imports",
    "clean": "tsx scripts/clean-compiled-files.ts",
    "validate": "tsx scripts/validate-plugins.ts",
    "validate-previews": "tsx scripts/validate-previews.ts --warn-only",
    "generate-metadata": "tsx scripts/generate-metadata.ts",
    "generate-types": "tsx scripts/generate-types.ts",
    "generate-preview-images": "tsx scripts/generate-preview-images.ts",
    "generate-section-previews": "tsx scripts/generate-section-previews.ts",
    "generate-tailwind-css": "tsx scripts/generate-tailwind-css.ts",
    "generate-style-index": "tsx scripts/generate-style-index.ts",
    "generate-duolingo-assets": "tsx scripts/generate-duolingo-assets.ts",
    "verify-no-relative-imports": "node scripts/verify-no-relative-imports.js",
    "copy-shared-css": "tsx scripts/copy-shared-css.ts",
    "dev": "tsc --watch --project tsconfig.dev.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "create-plugin": "tsx scripts/create-plugin.ts",
    "update-lastfm-mock": "tsx scripts/update-lastfm-mock-data.ts",
    "prepublishOnly": "pnpm validate && pnpm generate-metadata && pnpm generate-types && pnpm generate-section-previews && pnpm generate-tailwind-css && pnpm generate-style-index"
  }
}
