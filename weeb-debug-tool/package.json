{
  "name": "@weeb/debug-tool",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"vite,server\" -c \"cyan,magenta\" --kill-others-on-fail \"vite\" \"nodemon --exec tsx server/index.ts\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "server:watch": "tsx --watch server/index.ts"
  },
  "dependencies": {
    "@weeb/svg-generator": "workspace:*",
    "@weeb/weeb-plugins": "workspace:*",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.0",
    "vite": "^6.0.1"
  }
}

